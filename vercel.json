{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "redirects": [
    { "source": "/", "destination": "https://github.com/datejer/vercel-badge" }
  ],
  "functions": {
    "api/nikkiasteinza/twist-api.ts": {
      "includeFiles": "assets/**"
    }
  }
}